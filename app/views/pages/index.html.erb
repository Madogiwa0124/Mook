<h1>Pages</h1>
<p id="notice"><%= notice %></p>
<% @pages.each do |page| %>
  <p>名前：<%= page.name %></p>
  <p>URL：<%= page.url %></p>
  <p>
    <%= link_to 'Show', page %></td>
    <%= link_to 'Edit', edit_page_path(page) %></td>
    <%= link_to 'Destroy', page, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </p>
<% end %>

<%= link_to 'New Page', new_page_path %>
